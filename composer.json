{
    "name": "ogwebsolutions/laravel-form-builder",
    "description": "A customizable Drag & Drop form builder package for Laravel that allows you to create forms with ease.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "OG Web Solutions",
            "email": "support@ogwebsolutions.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "Ogwebsolutions\\Formbuilder\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ogwebsolutions\\Formbuilder\\FormBuilderServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "laravel",
        "formbuilder",
        "forms",
        "form builder",
        "drag and drop"
    ],
    "support": {
        "email": "support@ogwebsolutions.com",
        "issues": "https://github.com/OG-Web-Solutions/laravel-form-builder/issues",
        "source": "https://github.com/OG-Web-Solutions/laravel-form-builder"
    }
}
